{
    "devCommand": "uvicorn bootstrap.server:app --host 0.0.0.0 --port 8000",
    "builds": [
        {
            "src": "*.py",
            "use": "@devjin8/vercel-poetry@latest"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "main.py"
        }
    ]
}